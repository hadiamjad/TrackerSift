{
    "name": "NetworkCallStackCapture",
    "description": "Displays the live log with the http requests headers",
    "version": "0.7",
    "permissions": [
      "debugger"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "NetworkCallStackCapture"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "manifest_version": 2
  }